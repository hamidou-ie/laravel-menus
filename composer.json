{
  "name": "hamidou-ie/laravel-menus",
  "description": "Laravel Menu management (Laravel 11 compatible fork)",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0",
    "illuminate/config": "^11.0",
    "illuminate/view": "^11.0",
    "laravelcollective/html": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Nwidart\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nwidart\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "7.5-dev"
    },
    "laravel": {
      "providers": [
        "Nwidart\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "Nwidart\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
